html
	-ms-text-size-adjust: 100%
	-webkit-text-size-adjust: 100%

html, body
	font-size: 24px

body
	background-color: white
	color: black
	font-family: Baskerville
	line-height: 1.4
	
flex()
	display: flex
	flex-wrap: nowrap

flexRow()
	flex()
	flex-direction: row
	
flexColumn()
	flex()
	flex-direction: column
	
flexCenter()
	flex()
	justify-content: center
	align-content: center

